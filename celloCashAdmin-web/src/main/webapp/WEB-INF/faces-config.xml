<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    
    <name>primefaces_harmony</name>
    
    <component>
        <component-type>org.primefaces.component.HarmonyMenu</component-type>
        <component-class>org.primefaces.harmony.component.HarmonyMenu</component-class>
    </component>
    
    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.HarmonyMenuRenderer</renderer-type>
            <renderer-class>org.primefaces.harmony.component.HarmonyMenuRenderer</renderer-class>
        </renderer>
    </render-kit>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>success-login</from-outcome>
            <to-view-id>/dashboard.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>disconnect-login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
             <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/dashboard.xhtml</to-view-id>
             <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>changePassword</from-outcome>
            <to-view-id>/security/user/changePassword.xhtml</to-view-id>
             <redirect />
        </navigation-case>
        
        <!-- Security-->
        <navigation-case>
            <from-outcome>listUsers</from-outcome>
            <to-view-id>/security/user/listUsers.xhtml</to-view-id>
